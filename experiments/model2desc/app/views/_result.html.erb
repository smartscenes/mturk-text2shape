<tr>
  <td><%= result.id %></td>
  <td><%= result.updated_at %></td>
  <td><%= link_to result.item, params.merge(:item => result.item).except(*[:offset, :limit]) %></td>
  <td><%= link_to result.condition, params.merge(:condition => result.condition).except(*[:offset, :limit]) %></td>
  <td><%= link_to result.workerId, params.merge(:workerId => result.workerId).except(*[:offset, :limit]) %></td>
  <td>
    <% entry = JSON.parse(result.data) %>
    <% if entry %>
      <% preview_url = getImageUrl(result.item, 13) %>
      <% if preview_url %>
        <%= image_tag 'scenePreview.jpg', { 'data-original' => preview_url, 'class' => 'img-responsive lazy'} %>
      <% end %>
    <% end %>
  </td>
  <td>
    <% if entry %>
      <%= entry['description'] %>
    <% else %>
      <%= result.item %>
    <% end %>
  </td>
  <td>
    <%= render 'mturk/itemStatusButtons', :result => result %>
  </td>
</tr>